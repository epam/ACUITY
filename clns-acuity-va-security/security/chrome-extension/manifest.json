{
    "manifest_version": 2,
    "name": "Swap Acuity Credentials",
    "description": "This extension swaps the users credentials for the Acuity web application.",
    "version": "1.0",
    "page_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "cookies",
        "declarativeContent",
        "tabs",
        "storage",
        "http://*/*", "https://*/*"
    ]
}
