    "copyindexie": "copyfiles -f ./src/index_ie.* ./src/error.* dist",
    "pretest": "npm run copydefs",
    "test": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test",
    "test-local": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test -sr=false --reporters=mocha --browsers Chrome",
